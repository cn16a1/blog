                            <%= postHTML[2].title %>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <hr>
        <div class="card" style="max-width: 540px;">
            <div class="row no-gutters">
                <div class="col-md-4">
                    <img src="img/logo/<%= postHTML[3].logo %>" class="card-img" alt="...">
                </div>
                <div class="col-md-8">
                    <div class="card-title">
                        <a href="/post/<%= postHTML[3]._id %>" class="card-text">
                            <%= postHTML[3].title %>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="main">
        <div class="title">
            <span class="text-md bg-dark " style="width:110px">Last Post</span>
            <hr>
        </div>

        <% allPosts.forEach((el,index) => { %>

        <% if((allPosts.length - 1) == index){ %>
        <div class="card mb-3" style="max-width: 540px;">
            <div class="row no-gutters">
                <div class="col-md-4">
                    <img src="img/logo/<%= el.logo %> " class="card-img" alt="...">
                </div>
                <div class="col-md-8">
                    <div class="card-body">
                        <a href="/post/<%= el._id %>">
                            <h5 class="card-title"><%= el.title %></h5>
                            <p class="card-text">
                                <%= el.contentSub.substr(0,200) %>...
                            </p>
                            <p class="card-text"><small class="text-muted">Day:<%= el.date %></small></p>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <% } else{ %>
        <div class="card mb-3" style="max-width: 540px;">
            <div class="row no-gutters">
                <div class="col-md-4">
                    <img src="img/logo/<%= el.logo %> " class="card-img" alt="...">
                </div>
                <div class="col-md-8">
                    <div class="card-body">
                        <a href="/post/<%= el._id %>">
                            <h5 class="card-title"><%= el.title %></h5>
                            <p class="card-text">
                                <%= el.contentSub.substr(0,200) %>...
                            </p>
                            <p class="card-text"><small class="text-muted">Day:<%= el.date %></small></p>
                        </a>
                    </div>
                </div>
            </div>
        </div>
        <hr>
        <% } %>

        <% }); %>


        <nav aria-label="Page navigation example">
            <ul class="pagination justify-content-center">
                <li class="page-item">
                    <a class="page-link" href="?page=1">
                        First Page
                    </a>
                </li>
                <% if(currentPage > 1) { %>
                <li class="page-item">
                    <a class="page-link" href="?page=<%=+currentPage - 1%>">
                        <i class="fa fa-angle-double-left"></i>
                    </a>
                </li>
                <% }%>

                <% for(let i = 1 ; i <= allPage ; i++) { %>
                <li class="page-item"><a class="page-link" href="/?page=<%=i%>"><%= i %></a></li>
                <% } %>

                <% if(currentPage < allPage) { %>
                <li class="page-item">
                    <a class="page-link" href="?page=<%=+currentPage + 1%>">
                        <i class="fa fa-angle-double-right"></i>
                    </a>
                </li>
                <% }%>
                <li class="page-item"><a class="page-link" href="?page=<%=allPage%>">Last Page</a></li>
            </ul>
        </nav>
    </div>

